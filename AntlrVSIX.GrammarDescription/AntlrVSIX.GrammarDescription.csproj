<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/kaby76/AntlrVSIX</PackageProjectUrl>
    <RepositoryUrl>https://github.com/kaby76/AntlrVSIX</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>Antlr VS Extension</PackageTags>
    <Description>Contains interface used to describe a language for support in AntlrVSIX and Visual Studio IDE.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>Ken Domino</Authors>
    <Company />
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="PresentationCore" />
    <Reference Include="PresentationFramework" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Antlr4.Runtime.Standard">
      <Version>4.7.2</Version>
    </PackageReference>
  </ItemGroup>

</Project>
